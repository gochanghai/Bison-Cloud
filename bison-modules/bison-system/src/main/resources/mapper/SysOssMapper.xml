<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bison.system.mapper.SysOssMapper">

    <resultMap type="SysOss" id="SysOssResult">
        <id property="id" column="id"/>
        <result property="url" column="url"/>
        <result property="service" column="service"/>
        <result property="fileName" column="file_name"/>
        <result property="createBy" column="create_by"/>
        <result property="fileSuffix" column="file_suffix"/>
        <result property="createTime" column="create_time"/>
    </resultMap>

    <sql id="selectSysOssVo">
        select id, file_name, file_suffix, url, create_time, create_by, service
		from sys_oss
    </sql>

    <insert id="insertSelective">
        insert into sys_oss(
        <if test="id != null and id != 0">id,</if>
        <if test="service != null">service,</if>
        <if test="url != null and url != ''">url,</if>
        <if test="fileName != null and fileName != ''">file_name,</if>
        <if test="fileSuffix != null and fileSuffix != ''">file_suffix,</if>
        <if test="createBy != null and createBy != ''">create_by,</if>
        create_time
        )values(
        <if test="id != null and id != 0">#{id},</if>
        <if test="service != null">#{service},</if>
        <if test="url != null and url != ''">#{url},</if>
        <if test="fileName != null and fileName != ''">#{fileName},</if>
        <if test="fileSuffix != null and fileSuffix != ''">#{fileSuffix},</if>
        <if test="createBy != null and createBy != ''">#{createBy},</if>
        sysdate()
        )
    </insert>

    <update id="updateByPrimaryKeySelective">
        update sys_oss
        <set>
            <if test="service != null and service != ''">service = #{service},</if>
            <if test="url != null and url != ''">url = #{url},</if>
            <if test="fileName != null and fileName != ''">file_name = #{fileName},</if>
            <if test="fileSuffix != null and fileSuffix != ''">file_suffix = #{fileSuffix},</if>
            <if test="updateBy != null and updateBy != ''">update_by = #{updateBy},</if>
            update_time = sysdate()
        </set>
        where id = #{id}
    </update>

    <delete id="deleteByIds">
        delete from sys_oss where id in
        <foreach collection="list" item="id" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <select id="selectByExample" resultType="com.ruoyi.system.domain.SysOss">
        <include refid="selectSysOssVo"/>
        <where>
            <if test="fileName != null and fileName != ''">
                AND file_name like concat('%', #{fileName}, '%')
            </if>
            <if test="createBy != null and createBy != ''">
                AND create_by = #{createBy}
            </if>
            <if test="fileSuffix != null and fileSuffix != ''">
                AND file_suffix = #{fileSuffix}
            </if>
        </where>
    </select>

    <select id="selectByPrimaryKey" resultType="com.ruoyi.system.domain.SysOss">
        <include refid="selectSysOssVo"/>
        where id = #{id}
    </select>

</mapper>